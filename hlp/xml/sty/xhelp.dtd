<!--___________________________Elements_________________________-->

<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!ELEMENT help  (chapter*)>
<!ATTLIST help
    title   CDATA   #REQUIRED
    description CDATA   #REQUIRED
    default CDATA   #REQUIRED
    version CDATA   #IMPLIED
    date    CDATA   #IMPLIED
    xmlns   CDATA   #REQUIRED
    >



<!--~~~~~~~~~~~~~~~~~~~Sectioning~~~~~~~~~~~~~~~~~~~-->

<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!ELEMENT chapter   (intro?,print-toc+,HHobject*,section*)>
<!ATTLIST chapter
    name    CDATA   #REQUIRED
    title   CDATA   #REQUIRED
    keywords    CDATA   #REQUIRED
    description CDATA   #REQUIRED
    version CDATA   #IMPLIED
    date    CDATA   #IMPLIED
    >

<!ELEMENT section   (#PCDATA|subsection|%texttags;|%HHobjects;)*>
<!ATTLIST section
    name    CDATA   #REQUIRED
    title   CDATA   #REQUIRED
    keywords    CDATA   #REQUIRED
    description CDATA   #IMPLIED
    >

<!ELEMENT subsection    (#PCDATA|subsubsection|%texttags;|%HHobjects;)*>
<!ATTLIST subsection
    name    CDATA   #REQUIRED
    title   CDATA   #REQUIRED
    keywords    CDATA   ""
    >

<!ELEMENT subsubsection (#PCDATA|%texttags;|%HHobjects;)*>
<!ATTLIST subsubsection
    name    CDATA   #REQUIRED
    title   CDATA   #REQUIRED
    >


<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!ELEMENT intro (#PCDATA|%texttags;)*>
<!ATTLIST intro
    toc CDATA   "no"
    >

<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!ELEMENT print-toc (#PCDATA|%texttags;)*>
<!ATTLIST print-toc
    verbose CDATA   "no"
    >


